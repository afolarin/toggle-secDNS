{
  "name": "Secure DNS Toggle Button",
  "version": "1.0",
  "description": "A Chrome extension to toggle the Secure DNS setting.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}